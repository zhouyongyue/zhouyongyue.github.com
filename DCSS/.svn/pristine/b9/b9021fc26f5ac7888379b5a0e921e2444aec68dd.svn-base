.modify .cnt .right .you .icon{width:0;height:0;border-top:5px solid transparent;border-right:6px solid #fff;border-bottom:5px solid transparent;position:absolute;left:-6px;top:50%;margin-top:-5px}

.modify .cnt .right.dy{border:1px solid #23aecf;background:#23aecf}
