/*Developer*/
.developer{width:90%;padding-bottom:144px}
.developer>div .bt{font-size:30px;color:#23aecf;text-align:center;padding-top:110px}
.developer>div .sm{font-size:24px;color:#898989;text-align:center}
.developer .top .powerList{padding:30px 0 20px;overflow:hidden}
.developer .top .powerList>li{float:left;margin-right:1.3%;margin-bottom:10px;width:24%}
.developer .top .powerList>li:nth-child(4n){margin-right:0}
.developer .top .powerList>li img{display:block;width:100%}
.developer>div .applyNow{display:block;width:25%;height:66px;text-align:center;line-height:66px;background:#23aecf;color:#fff;font-size:30px;margin:0 auto}
.developer .bottom .influeList{overflow:hidden;padding:30px 0 20px}
.developer .bottom .influeList>li{position:relative;width:49%;float:left;margin-right:2%;margin-bottom:20px}
.developer .bottom .influeList>li:nth-child(even){margin-right:0}
.developer .bottom .influeList>li .img img{display:block;width:100%}
.developer .bottom .influeList>li .tu{position:absolute}
.developer .bottom .influeList>li .tu01{right:12px;bottom:12px}
.developer .bottom .influeList>li .tu02{bottom:12px;left:12px}
.developer .bottom .influeList>li .tu03{top:12px;right:12px}
.developer .bottom .influeList>li .tu04{top:12px;left:12px}
@media screen and (max-width:768px){
    .developer>div .bt{padding-top:2rem;font-size:20px}
    .developer>div .sm{font-size:18px}
    .developer>div .applyNow{width:100%;height:2rem;line-height:2rem;font-size:16px}
    .developer .top .powerList>li{width:49%;margin-right:2%}
    .developer .top .powerList>li:nth-child(4n){margin-right:4%}
    .developer .top .powerList>li:nth-child(2n){margin-right:0}
    .developer .bottom .influeList>li{width:100%;margin-right:0}
    .developer .bottom .influeList>li .tu02{right:12px;left:auto}
    .developer .bottom .influeList>li .tu03{top:auto;bottom:12px}
    .developer .bottom .influeList>li .tu04{top:auto;left:auto;bottom:12px;right:12px}
}

/*产品列表*/
.por01{padding-top:12px;overflow: hidden}
.por01 a{display:table;width:24%;height:334px;text-align:center;margin-right:1.3%;background:url(../images/porListBg.png) center no-repeat;float:left}
.por01 a div{display:table-cell;vertical-align:middle}
.por01 a:nth-child(4n){margin-right:0}
.por01 a img{display:block;margin:0 auto 40px}
.por01 a span{font-size:30px;color:#898989;font-weight:700}
.por01 a.active span{color:#23aecf}

.porduct{width:90%}
.porduct .list{margin-bottom:60px;overflow:hidden}
.porduct .bt{font-size:30px;color:#23aecf;height:100px;line-height:130px;border-bottom:1px solid #e0dfdf}
.porduct .list>li{width:24%;height:430px;margin-right:1.3%;float:left;text-align:center;margin-top:40px}
.porduct .list>li:nth-child(4n){margin-right:0}
.porduct .list>li img{display:block;width:80%;margin:40px auto}
.porduct .list>li .tm{font-size:18px;color:#4c4948;font-weight:700}
.porduct .list>li .sm{font-size:16px;color:#898989;margin:10px auto 15px;width:75%}
.porduct .list>li .readMore{display:block;width:50%;height:32px;line-height:30px;color:#898989;font-size:18px;border:1px solid #c3c2c2;margin:0 auto}
.porduct .list>li:hover{background:#eaebeb}
.porduct .list>li:hover .sm,.porduct .list>li:hover .tm{color:#23aecf}
.porduct .list>li .readMore:hover{background:#23aecf;color:#fff;border:1px solid #23aecf}
@media screen and (max-width:1400px){
    .porduct .list>li{height:380px}
