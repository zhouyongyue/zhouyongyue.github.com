}
@media screen and (max-width:768px){
    .por01 a{display:table;width:49%;height:8rem;margin-right:2%;}
    .por01 a:nth-child(even){margin-right:0}
    .por01 a img{max-width: 90%;margin-bottom: 0.5rem}
    .por01 a span{font-size: 0.6rem}
    .porduct .bt{font-size:0.8rem}
    .porduct{width: 100%;padding:0 0.5rem}
    .porduct .list>li{width:48%;height:auto;margin-right:4%;}
    .porduct .list>li:nth-child(3n){margin-right:4%;}
    .porduct .list>li:nth-child(even){margin-right:0;}
    .porduct .list>li img{width:90%;}
    .porduct .list>li .sm{width:90%}
    .porduct .list>li .readMore{width:80%;}
}
