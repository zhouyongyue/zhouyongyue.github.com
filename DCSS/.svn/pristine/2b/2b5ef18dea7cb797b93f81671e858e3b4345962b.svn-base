.about .tour .cnt img{width:100%;display:block}
.about .bus .cnt{margin:90px auto 110px;overflow:hidden;width:80%}
.about .bus .cnt a{float:left;width:16%;border-radius:50%;overflow:hidden;margin:0 4.5% 20px}
.about .bus .cnt a img{display:block;width:100%}
@media screen and (max-width:768px){
    .about{width:100%;padding:0 0.5rem}
    .about .tit{font-size:0.75rem;height:3rem;line-height:3.5rem}
    .about .tour .cnt{margin-bottom:0.75rem}
    .about .bus .cnt{margin:1rem auto 2rem;width:100%}
    .about .bus .cnt a{width:20%;margin:0 2.5% 0.5rem}
}

