@charset "utf-8";
.TLcenter{-webkit-transform:translate(-50%,-50%);-ms-transform:translate(-50%,-50%);-moz-transform:translate(-50%,-50%);transform:translate(-50%,-50%)}
.NoSelect{-moz-user-select:none;-webkit-user-select:none;-ms-user-select:none;-khtml-user-select:none;user-select:none}
/*内页banner*/
.page-banner{width:100%;display:block}
/*Influencer*/
.flue{padding-top:95px;}
.flue .prompt{color:#898989;text-align:center;font-size:18px;}
.flue .subForm{overflow:hidden;margin:78px auto 0;width:90%;max-width:1220px;}
.flue .subForm .left{float:left;margin-top:24px;padding-right:50px;}
.flue .subForm .left img{display:block;margin-bottom:68px;}
.flue .subForm .right{overflow:hidden;margin-left:118px;}
.flue .subForm .right .half{float:left;margin-right:2%;width:49%;}
.flue .subForm .right .half input{padding:10px;width:100%;height:50px;border:1px solid #c9caca;background:#c9caca;}
.flue .subForm .right .half:nth-child(2){margin-right:0;}
.flue .subForm .right .full{float:left;width:100%;}
.flue .subForm .right .full input{padding:10px;width:100%;height:50px;border:1px solid #c9caca;background:#c9caca;}
.flue .subForm .right .wz{height:50px;color:#898989;font-size:18px;line-height:60px;}
.flue .subForm .sub{display:block;margin:70px auto 120px;width:57%;height:66px;background:#23aecf;color:#fff;font-size:30px;}
.flue .subForm .sub:hover{background:#12a1c3}
@media screen and (max-width:768px){
    .flue{padding:1rem .5rem 0}
    .flue .subForm{margin:1rem 0 0;width:100%}
    .flue .subForm .left{float:none;margin:.5rem 0 0;padding-right:0;overflow:hidden}
    .flue .subForm .left img{width:2rem;float:left;margin:0 .6rem .5rem 0}
    .flue .subForm .left img:last-child{margin-right:0}
    .flue .subForm .right{margin-left:0}
    .flue .subForm .right .wz{height:2rem;line-height:2.3rem;font-size:.6rem}
    .flue .subForm .right .half input{height:2rem}
    .flue .subForm .right .full input{height:2rem}
    .flue .subForm .sub{margin:1.5rem auto 4rem;width:57%;height:2rem;font-size:.75rem}
}

/*下载详情*/
.downDetails{width:90%;padding-bottom:80px}
.downDetails .tit{font-size:32px;color:#23aecf;padding:110px 0 15px;border-bottom:1px solid #dcdbdb}
.downDetails .model{font-size:30px;color:#3e3a39;padding:40px 0 20px}
.downDetails .product{overflow:hidden}
.downDetails .product .tu{float:left;width:578px}
.downDetails .product .introd{margin-left:578px;padding-left:39px}
.downDetails .product .introd .bt{font-size:30px;color:#3e3a39;font-weight:700;margin-bottom:45px}
.downDetails .product .introd .nr{font-size:18px;color:#898989;line-height:30px}
.downDetails .list>li{padding-top:60px;line-height:35px}
.downDetails .list>li .bt{font-size:24px;color:#3e3a39;padding-bottom:20px}
.downDetails .list>li .xq{font-size:18px;color:#898989}
.downDetails .list>li .xq span{margin-right:60px}
.downDetails .list>li .xq a{color:#23aecf}
.downDetails .list>li .xq a:hover{text-decoration:underline}
@media screen and (max-width:1024px){
    .downDetails .product .tu{float:none;max-width:100%;display:block}
    .downDetails .product .introd{margin:20px 0 0;padding-left:0}
    .downDetails .product .introd .bt{margin-bottom:20px}
    .downDetails .list>li .bt{font-size:18px;padding-bottom:10px}
    .downDetails .list>li .xq span{margin-right:20px;font-size:14px}
}
@media screen and (max-width:768px){
    .downDetails .tit{padding:2rem 0 .5rem}
    .downDetails .model{font-size:.9rem}
    .downDetails .product .introd .bt{font-size:.8rem}
    .downDetails .list>li{padding-top:1.5rem}
    .downDetails .list>li .xq{margin-top:.5rem}
    .downDetails .list>li .xq span{display:block}
}

/*Supports*/
.suppor{padding-top:55px;width:90%}
.suppor .tit{color:#23aecf;font-size:30px;padding-bottom:15px;border-bottom:1px solid #dcdddc}
.suppor .tit span{color:#898989;font-size:18px;margin-left:24px}
.suppor .list{overflow:hidden;padding-top:42px}
.suppor .list a{float:left;width:15%;height:244px;margin-right:2%;margin-bottom:13px;text-align:center}
.suppor .list a:nth-child(6n){margin-right:0}
.suppor .list a .img{height:180px;line-height:180px;overflow:hidden}
.suppor .list a .img img{vertical-align:middle;max-width:100%}
.suppor .list a span{font-size:18px;color:#3e3a39;font-weight:700}
.suppor .list a:hover{background:#efefef}
.suppor .list a:hover .img img{-webkit-transform:scale(1.1);transform:scale(1.1);-webkit-transition:.6s;transition:.6s}
.suppor .list a:hover span{font-size:24px;color:#23aecf}
.suppor .ser{padding-bottom:166px}
.suppor .ser .modular .bt{font-size:30px;color:#23aecf;padding:100px 0 15px;border-bottom:1px solid #dbdcdc}
.suppor .ser .modular .nr{margin-top:44px;position:relative}
.suppor .ser .modular .nr img{max-width:100%;display:block}
.suppor .ser .modular .nr .email{position:absolute;top:50%;right:14%;-webkit-transform:translate(0,-50%);-ms-transform:translate(0,-50%);-moz-transform:translate(0,-50%);transform:translate(0,-50%)}
.suppor .ser .modular .nr .email a{color:#9fa0a0;font-size:17px;line-height:40px}
.suppor .ser .modular .nr .email a .img{display:inline-block;width:30px;height:20px;vertical-align:middle;margin-top:-2px;margin-right:12px;background:url(../images/email01.png) no-repeat center/cover;-webkit-transition:.3s;transition:.3s}
.suppor .ser .modular .nr .email a:hover{color:#23aecf}
.suppor .ser .modular .nr .email a:hover .img{background:url(../images/email02.png) no-repeat center/cover}
@media screen and (max-width:1400px){
    .suppor .list a{width:22%;margin-right:4%;}
    .suppor .list a:nth-child(6n){margin-right:4%}
    .suppor .list a:nth-child(4n){margin-right:0}
}
@media screen and (max-width:1024px){
    .suppor .list a{width:30%;margin-right:5%;}
    .suppor .list a:nth-child(6n){margin-right:5%}
    .suppor .list a:nth-child(4n){margin-right:5%}
    .suppor .list a:nth-child(3n){margin-right:0}
}
@media screen and (max-width:768px){
    .suppor{padding-top:1rem}
    .suppor .list{padding-top:0.8rem}
    .suppor .list a{width:45%;margin-right:10%;height:7rem}
    .suppor .list a:nth-child(3n){margin-right:10%}
    .suppor .list a:nth-child(even){margin-right:0%}
    .suppor .list a .img{height:5rem;line-height:5rem}
    .suppor .ser{padding-bottom:4rem}
    .suppor .ser .modular .bt{padding:2.5rem 0 0.5rem}
    .suppor .ser .modular .nr{margin-top:1rem;}
    .suppor .ser .modular .nr .email{position:static;-webkit-transform:translate(0,0);-ms-transform:translate(0,0);-moz-transform:translate(0,0);transform:translate(0,0)}
    .suppor .ser .modular .nr .email a{line-height:2rem}
    .suppor .ser .modular .nr .email a .img{width:.75rem;height:.5rem;margin-top:-.1rem;margin-right:.26rem}
    .suppor .ser .modular .nr .email a:hover{color:#9fa0a0}
    .suppor .ser .modular .nr .email a:hover .img{background:url(../images/email01.png) no-repeat center/cover}
}

/*about*/
.about{width:90%}
.about .tit{color:#23aecf;font-size:30px;text-align:center;height:124px;line-height:160px}
.about .vdo .cnt{position:relative}
.about .vdo .cnt video{width:100%;display:block}
