/*会员信息修改*/
.modify{width:90%}
.modify .tit{color:#23aecf;font-size:30px;padding:110px 0 15px;border-bottom:1px solid #e6e6e6}
.modify .cnt{padding:43px 0 170px;overflow:hidden;position:relative}
.modify .cnt .left{width:286px;float:left;border:1px solid #cdcdcd}
.modify .cnt .left .tx{text-align:center;height:288px;line-height:282px}
.modify .cnt .left .tx img{vertical-align:middle;max-width:100%}
.modify .cnt .left .change{width:calc(100% + 2px);height:44px;text-align:center;line-height:44px;margin-left:-1px;background:#a5a5a5;font-size:18px;color:#fff;display:block;cursor:pointer;display:none}
.modify .cnt .left .change input{display:none}

.modify .cnt .right{width:240px;height:48px;line-height:48px;border-radius:50px;float:right;overflow:hidden;cursor:pointer;border:1px solid #a5a5a5;background:#a5a5a5;}
.modify .cnt .right .zuo{float:left;width:143px;height:100%;color:#fff}
.modify .cnt .right .zuo img{vertical-align:middle;margin:-2px 0 0 12px}
.modify .cnt .right .zuo .subscribe{padding-left:8px;display:none}
.modify .cnt .right .zuo .no-subscribe{display:none;padding-left:14px;display:inline}
.modify .cnt .right .you{float:right;width:95px;text-align:center;font-size:16px;color:#a5a5a5;position:relative;background:#fff}
.modify .cnt .right .you .icon{width:0;height:0;border-top:5px solid transparent;border-right:6px solid #fff;border-bottom:5px solid transparent;position:absolute;left:-6px;top:50%;margin-top:-5px}

.modify .cnt .right.dy{border:1px solid #23aecf;background:#23aecf}
.modify .cnt .right.dy .zuo .subscribe{display:inline}
.modify .cnt .right.dy .zuo .no-subscribe{display:none}

.modify .cnt .center{margin:0 340px}
.modify .cnt .center .modForm{overflow:hidden}
.modify .cnt .center .modForm .lab{width:46%;float:left;margin-right:8%;margin-bottom:30px}
.modify .cnt .center .modForm .lab:nth-child(even){margin-right:0}
.modify .cnt .center .modForm .lab p{height:24px;line-height:18px;font-size:18px;color:#898989}
.modify .cnt .center .modForm .lab input{float:left;width:100%;height:50px;border:1px solid #d8d8d8;padding:10px;background:#eee}
.modify .cnt .center .modForm .lab .select{width:100%;height:50px;border:1px solid #d8d8d8;background:#eee;position:relative}
.modify .cnt .center .modForm .lab .select .value{height:100%;line-height:48px;padding-left:10px;font-size:18px;color:#00b7d6;position:relative}
.modify .cnt .center .modForm .lab .select .value i{position:absolute;width:25px;height:50px;top:-1px;right:-1px;background:#a5a5a5 url(../images/icon-select.png) no-repeat center/60% 25%}
.modify .cnt .center .modForm .lab .select .selected{display:none;position:absolute;top:50px;left:-1px;width:calc(100% + 2px)}
.modify .cnt .center .modForm .lab .select .selected li{height:54px;line-height:52px;padding-left:10px;border:1px solid #d8d8d8;border-top:none;background:#f6f6f7;font-size:18px;color:#898989}
.modify .cnt .center .modForm .password{position:relative}
.modify .cnt .center .modForm .password .btn{width:auto;height:50px;font-size:18px;color:#fff;padding:0 8px;position:absolute;top:24px;right:-1px;background:#a5a5a5;border:none;display:none}
.modify .cnt .center .btn{text-align:center;padding-top:40px}
.modify .cnt .center .btn .modifyBtn{display:block;width:54%;height:66px;color:#fff;font-size:30px;background:#00b7d6;margin:0 auto}
.modify .cnt .center .btn .ipt{width:38%;height:66px;font-size:30px;display:none}
.modify .cnt .center .btn .save{margin-right:8%;color:#fff;background:#00b7d6}
.modify .cnt .center .btn .cancel{border:1px solid #c9caca;color:#898989;background:#fbfaf9}

.modify .cnt.modifyStrat .left .change{display:block}
.modify .cnt.modifyStrat .center .modForm .password .btn{display:block}
.modify .cnt.modifyStrat .center .btn .modifyBtn{display:none}
.modify .cnt.modifyStrat .center .btn .ipt{display:inline-block}
@media screen and (max-width:1400px){
