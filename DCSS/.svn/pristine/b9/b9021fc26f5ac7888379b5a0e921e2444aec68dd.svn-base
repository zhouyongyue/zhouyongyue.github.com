.modify .cnt .center{margin:0 340px}
.modify .cnt .center .modForm{overflow:hidden}
.modify .cnt .center .modForm .lab{width:46%;float:left;margin-right:8%;margin-bottom:30px}
.modify .cnt .center .modForm .lab:nth-child(even){margin-right:0}
.modify .cnt .center .modForm .lab p{height:24px;line-height:18px;font-size:18px;color:#898989}
.modify .cnt .center .modForm .lab input{float:left;width:100%;height:50px;border:1px solid #d8d8d8;padding:10px;background:#eee}
.modify .cnt .center .modForm .lab .select{width:100%;height:50px;border:1px solid #d8d8d8;background:#eee;position:relative}
.modify .cnt .center .modForm .lab .select .value{height:100%;line-height:48px;padding-left:10px;font-size:18px;color:#00b7d6;position:relative}
.modify .cnt .center .modForm .lab .select .value i{position:absolute;width:25px;height:50px;top:-1px;right:-1px;background:#a5a5a5 url(../images/icon-select.png) no-repeat center/60% 25%}
.modify .cnt .center .modForm .lab .select .selected{display:none;position:absolute;top:50px;left:-1px;width:calc(100% + 2px)}
.modify .cnt .center .modForm .lab .select .selected li{height:54px;line-height:52px;padding-left:10px;border:1px solid #d8d8d8;border-top:none;background:#f6f6f7;font-size:18px;color:#898989}
.modify .cnt .center .modForm .password{position:relative}
.modify .cnt .center .modForm .password .btn{width:auto;height:50px;font-size:18px;color:#fff;padding:0 8px;position:absolute;top:24px;right:-1px;background:#a5a5a5;border:none;display:none}
.modify .cnt .center .btn{text-align:center;padding-top:40px}
.modify .cnt .center .btn .modifyBtn{display:block;width:54%;height:66px;color:#fff;font-size:30px;background:#00b7d6;margin:0 auto}
