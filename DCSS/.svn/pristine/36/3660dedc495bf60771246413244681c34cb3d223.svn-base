<script>

    var swiper = new Swiper('.gallery-top', {
        nextButton: '.swiper-button-next',
        prevButton: '.swiper-button-prev',
        spaceBetween: 10,
        speed:800
    });

    $(function(){

        $('.img-box .list').each(function(i){
            $(this).hover(function(){
                $(this).addClass('on').siblings().removeClass('on')
                swiper.slideTo(i,1000,false)
            })
        })

        $('.swiper-button-next,.swiper-button-prev').on('click',function(){
            var index = swiper.activeIndex;
            $('.img-box .list').eq(index).addClass('on').siblings().removeClass('on');
        })
            
    })

    //自定义下拉框
    $(".place .value").on("click",selected);
    function selected(){
        $(this).next(".selected").slideToggle(0);
        return false;
    };
    $(".place .selected li").on("click",function(){
        $(this).parent(".selected").slideToggle(0);
        $(this).parent(".selected").prev().children("span").html($(this).html());
        return false;
    });
    $(document).on("click",function(){
        $(".place .selected").slideUp(0);
    });

    $(function(){
        $('.index-item03 > *').on('click',function(){
            $('.bg000,.video-modal').show();
        })
        $('.bg000').on('click',function(){
            $('.bg000,.video-modal').hide();
        })
    })




</script>
